{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
      "prettier",
        "plugin:prettier/standard",
        "plugin:prettier",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
