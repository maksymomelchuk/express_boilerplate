{
  // "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "importOrder": ["<THIRD_PARTY_MODULES>", "^[a-zA-Z]\\w*\\/.*$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}